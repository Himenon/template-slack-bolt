{
  "name": "@himenon/template-slack-bolt",
  "version": "1.1.1",
  "description": "ESModule Library Template",
  "keywords": [
    "template",
    "typescript"
  ],
  "homepage": "https://github.com/Himenon/template-slack-bolt#readme",
  "bugs": {
    "url": "https://github.com/Himenon/template-slack-bolt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/template-slack-bolt.git"
  },
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://github.com/Himenon"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "dist": "dist-server"
  },
  "files": [
    "dist-server",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "pnpm run develop",
    "develop": "pnpm run ts:esm ./src/entry.ts",
    "build": "webpack",
    "watch": "nodemon",
    "format": "run-s prettier:format eslint:format",
    "test": "run-p test:vitest eslint:validate prettier:validate",
    "prettier:format": "prettier \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md,html}\" --write ./src",
    "prettier:validate": "prettier --check \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md,html}\"",
    "eslint:format": "eslint \"**/*.{ts,tsx}\" --fix",
    "eslint:validate": "eslint \"**/*.{ts,tsx}\"",
    "test:vitest": "vitest",
    "ts:esm": "node --loader ts-node/esm --experimental-specifier-resolution=node"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,yml,yaml,html,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "nodemonConfig": {
    "delay": 600,
    "exec": "pnpm run develop",
    "ext": "ts,tsx",
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "watch": [
      "src/**/*"
    ]
  },
  "dependencies": {
    "@himenon/message-command-parser": "^1.1.3",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@himenon/github-api-create-commit": "^2.0.0",
    "@himenon/openapi-parameter-formatter": "^0.2.2",
    "@slack/bolt": "^3.9.0",
    "@swc/core": "^1.2.197",
    "@types/node": "17.0.40",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "bufferutil": "^4.0.6",
    "compression": "^1.7.4",
    "conventional-changelog-angular-all": "1.7.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "escape-string-regexp": "^5.0.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-to-mrkdwn": "^3.0.0",
    "lint-staged": "13.0.0",
    "mdast-util-to-markdown": "^1.2.6",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "remark": "^14.0.2",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "remark-toc": "^8.0.1",
    "semver": "^7.3.5",
    "simple-git-hooks": "^2.8.0",
    "slackify-markdown": "^4.3.1",
    "sort-package-json": "1.57.0",
    "swc-loader": "^0.2.1",
    "ts-node": "^10.8.1",
    "tslib": "^2.3.1",
    "typescript": "4.7.3",
    "unified": "^10.1.2",
    "url-join": "^4.0.1",
    "utf-8-validate": "^5.0.8",
    "vitest": "^0.14.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "pnpm": ">=7"
  },
  "publishConfig": {
    "access": "public"
  }
}
